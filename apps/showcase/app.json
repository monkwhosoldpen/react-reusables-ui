{
  "expo": {
    "name": "React Native Reusables Showcase",
    "slug": "showcase",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "example",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": ""
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reactnativereusables.showcase"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#18181B"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "name": "React Native Reusables Showcase",
      "manifest": {
        "short_name": "PushPWA",
        "name": "Push Only PWA",
        "start_url": "/",
        "display": "standalone",
        "theme_color": "#18181B",
        "background_color": "#18181B",
        "orientation": "portrait",
        "icons": [
          {
            "src": "./assets/images/favicon.png",
            "sizes": "64x64 32x32 24x24 16x16",
            "type": "image/png"
          },
          {
            "src": "./assets/images/icon.png",
            "type": "image/png",
            "sizes": "192x192"
          },
          {
            "src": "./assets/images/adaptive-icon.png",
            "type": "image/png",
            "sizes": "512x512"
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "00772e2a-615c-4c12-93a4-8f5a08be8402"
      }
    },
    "owner": "monkwhosoldpen"
  }
}